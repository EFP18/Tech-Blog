<!-- Loop over each object in the projects array -->
{{#each post }}
<div class="row mb-4 project">
  <div class="col-md-5">
    <h2>

      <a href="/post/{{post.id}}">{{post.title}}</a>
    </h2>
  </div>
  <div class="col-md-7">
    <p>
      {{format_date date_created}}
    </p>
  </div>
</div>
{{/each}}
