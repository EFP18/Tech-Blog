<!-- Loop over each object in the posts array -->
<div class="col-md-6 post-list list-group">
  <h2>Your posts:</h2>
  {{#each posts}}
    <div class="row mb-2 card">
      <div class="col-md-8 card-header flex-row justify-space-between">
        <h4><a href="/dashboard/edit/{{id}}">{{title}}</a></h4>
        <div> {{format_date createdAt}} </div>
      </div>
      <div class="card-body">
        {{body}}
      </div>
    </div>
  {{/each}}
</div>